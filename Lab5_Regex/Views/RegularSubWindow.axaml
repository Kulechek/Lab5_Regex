<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="93"
                MaxWidth="300" MaxHeight="93"
                MinWidth="300" MinHeight="93"
        x:Class="Lab5_Regex.Views.RegularSubWindow"
        Title="OpenFilesWindow">



  <StackPanel>
    <TextBox Name="regTextBox" Text="{Binding Regex}" Watermark="Enter regular"/>
    <Button Name="okButton" Classes="button" HorizontalAlignment="Stretch" HorizontalContentAlignment="Center">OK</Button>
    <Button Name="cancelButton" Classes="button" HorizontalAlignment="Stretch" HorizontalContentAlignment="Center">Cancel</Button>
  </StackPanel>
  
</Window>
